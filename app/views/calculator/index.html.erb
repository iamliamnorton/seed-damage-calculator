<% content_for :title, "Seed Damage Calculator" %>
<% content_for :description, "Calculate the optimal fertilizer material to apply with seed." %>
<%= currently_at 'Calculator' %>

<h1><%= t('.title') %></h1>
<hr>

<%= render 'layouts/social_links' %>

<p><%= t('.description')%></p>

<div class="container">
  <div class="row">
    <div class="span6">
      <section id="input">
      <%= render 'input_form' %>
      </section>
    </div>
    <div class="span6">
      <section id='output'>
      <%= render 'output' %>
      </section>
    </div>
  </div>
</div>
